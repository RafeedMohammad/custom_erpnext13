 "html": "<div>{% set salary_structure_assignment_id = frappe.db.get_value('Salary Structure Assignment', {'employee': doc.employee}, 'salary_structure') %}</div>\n<div>{% set salary_structure_document = frappe.get_doc('Salary Structure', salary_structure_assignment_id) %}</div>\n<div>{% set earnings_table = salary_structure_document.earnings%}</div>\n\n<div>{% set district_name_in_bangla = frappe.db.get_value('District', {'district': doc.district}, 'district_name_in_bangla') %}</div>\n\n<div>{% set company_name_in_bangla = frappe.db.get_value('Company', {'name': doc.company}, 'company_name_in_bangla') %}</div>\n<div>{% set address_in_bangla = frappe.db.get_value('Company', {'name': doc.company}, 'address_in_bangla') %}</div>\n\n\n<header>\n    <h2>{{company_name_in_bangla}}</h2>\n    <h3>{{address_in_bangla}}</h3>\n    <p>_____________________________________________________________________________________________</p>\n</header>\n\n<br><br>\n\n<table id=\"employee\" style=\"width:100%\">\n    \n    <tr >\n      <td>\u09a8\u09be\u09ae: </td>\n       <td>{{doc.name_in_bangla}}</td> \n       <td>\u09aa\u09a6\u09ac\u09c0</td>\n       <td>{{doc.designation}}</td>\n       <td> \u09ac\u09b0\u09cd\u09a4\u09ae\u09be\u09a8 \u09a0\u09bf\u0995\u09be\u09a8\u09be  </td>\n       <td>{{doc.present_address_in_bangla}}</td>\n    </tr>\n\n\n   <tr >\n      <td>\u09aa\u09bf\u09a4\u09be\u09b0 \u09a8\u09be\u09ae: </td>\n       <td>{{doc.father_name_in_bangla}}</td> \n       <td>\u09b8\u09c7\u0995\u09b6\u09a8</td>\n       <td>{{doc.section}}</td>\n       <td>\u09ac\u09bf\u09ad\u09be\u0997</td>\n       <td>{{doc.division}}</td>\n    </tr>\n    \n     <tr >\n      <td>\u09ae\u09be\u09a4\u09be\u09b0 \u09a8\u09be\u09ae: </td>\n       <td>{{doc.mother_name_in_bangla}}</td> \n       <td>\u09ac\u09cd\u09af\u0995\u09cd\u09a4\u09bf\u0997\u09a4/\u099c\u09be\u09a4\u09c0\u09df \u09aa\u09b0\u09bf\u099a\u09df\u09aa\u09a4\u09cd\u09b0 \u09a8\u0982</td>\n       <td>{{doc.national_id}}</td>\n       <td>\u099c\u09c7\u09b2\u09be</td>\n       <td>{{district_name_in_bangla}}</td>\n    </tr>\n\n</table>\n\n\n\n<div>\n    <p>\n    \u099c\u09a8\u09be\u09ac/\u099c\u09a8\u09be\u09ac\u09be,<br>\n    &nbsp;&nbsp;&nbsp;&nbsp;\u0989\u09aa\u09b0\u09cb\u0995\u09cd\u09a4 \u09aa\u09a6\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u0986\u09aa\u09a8\u09be\u09b0 \u0986\u09ac\u09c7\u09a6\u09a8\u09aa\u09a4\u09cd\u09b0 \u0993 \u09b8\u09be\u0995\u09cd\u09b7\u09be\u09ce\u0995\u09be\u09b0\u09c7\u09b0 \u09aa\u09b0\u09bf\u09aa\u09cd\u09b0\u09c7\u0995\u09cd\u09b7\u09bf\u09a4\u09c7 \u0986\u09aa\u09a8\u09be\u0995\u09c7 \u0989\u09b2\u09cd\u09b2\u09c7\u0996\u09bf\u09a4 \u09aa\u09a6\u09c7 \u0993 \u0997\u09cd\u09b0\u09c7\u09a1\u09c7 \u09a8\u09bf\u09af\u09c1\u0995\u09cd\u09a4 \u0995\u09b0\u09be \u09b9\u09b2\u09cb\u0964 \u098f\u0987 \u09a8\u09bf\u09af\u09c1\u0995\u09cd\u09a4\u09bf \u0989\u09b2\u09cd\u09b2\u09c7\u0996\u09bf\u09a4 \u09a4\u09be\u09b0\u09bf\u0996 \u09b9\u09a4\u09c7 \u09a8\u09bf\u09ae\u09cd\u09a8\u09c7 \u09ac\u09b0\u09cd\u09a3\u09bf\u09a4 \u09b6\u09b0\u09cd\u09a4 \u09b8\u09be\u09aa\u09c7\u0995\u09cd\u09b7\u09c7 \u0995\u09be\u09b0\u09cd\u09af\u0995\u09b0\u09c0 \u09b9\u09ac\u09c7\u0964 <br><br>\n    </p>\n    <b>\u099a\u09be\u0995\u09c1\u09b0\u09c0\u09b0 \u09b6\u09b0\u09cd\u09a4 \u0993 \u09a8\u09bf\u09df\u09ae\u09be\u09ac\u09b2\u09c0\u0983</b>\n   \n</div>\n\n<div>{% set employee_base = frappe.db.get_value('Salary Structure Assignment', {'employee': doc.employee}, 'base') %}</div>\n\n\n\n<div class=\"a\">\n\n <table class=\"salary_table\" style=\"width: 100%\">\n   <tbody>\n   <tr >\n      <td style=\"width:40%\">\u09ae\u09c2\u09b2 \u09ac\u09c7\u09a4\u09a8 </td>\n       <td colspan=\"2\">Tk. {{((employee_base-1800)*2/3)|round(2, 'common')}}</td> \n    </tr>\n   <tr >\n      <td style=\"width:40%\">\u0997\u09c3\u09b9 \u09ad\u09be\u09a4\u09be  </td>\n       <td colspan=\"2\">Tk. {{((employee_base-1800)*1/3)|round(2, 'common')}}</td> \n    </tr>\n    \n    <tr >\n      <td style=\"width:40%\">\u09ae\u09c7\u09a1\u09bf\u0995\u09c7\u09b2 \u09ad\u09be\u09a4\u09be</td>\n       <td colspan=\"2\">Tk. 350</td> \n    </tr>\n    \n    <tr >\n      <td style=\"width:40%\">\u0996\u09be\u09a6\u09cd\u09af \u09ad\u09be\u09a4\u09be</td>\n       <td colspan=\"2\">Tk. 900</td> \n    </tr>\n    \n    <tr >\n      <td style=\"width:40%\">\u09aa\u09b0\u09bf\u09ac\u09b9\u09a8 \u09ad\u09be\u09a4\u09be</td>\n       <td colspan=\"2\">Tk. 600</td> \n    </tr>\n    </tbody>\n    <tr>\n      <td style=\"width:40%\">\u09ae\u09cb\u099f \u09ac\u09c7\u09a4\u09a8: </td>\n      <td>Tk. {{employee_base}}</td>\n    </tr>\n</table>\n\n</div>\n\n\n\n<div id = \"rules_and_regulations\">\n    <br><br>\n    <p>\n\n    <b>\u0995) \u09a6\u09c8\u09a8\u09bf\u0995 \u0995\u09b0\u09cd\u09ae\u0998\u09a8\u09cd\u099f\u09be\u0983</b> \u09ee \u0998\u09a3\u09cd\u099f\u09be, \u0996) \u0996\u09be\u09ac\u09be\u09b0 \u09ac\u09bf\u09b0\u09a4\u09bf \u09aa\u09cd\u09b0\u09a4\u09bf \u0995\u09b0\u09cd\u09ae\u09a6\u09bf\u09ac\u09b8\u09c7 \u09e7 \u0998\u09a3\u09cd\u099f\u09be (\u09e7-\u09e8 \u099f\u09be), \u0997) \u09a6\u09c8\u09a8\u09bf\u0995 \u0993\u09ad\u09be\u09b0\u099f\u09be\u0987\u09ae \u09b6\u09cd\u09b0\u09ae\u09bf\u0995\u09c7\u09b0 \u09b8\u09ae\u09cd\u09ae\u09a4\u09bf\u0995\u09cd\u09b0\u09ae\u09c7 \u09b8\u09b0\u09cd\u09ac\u09cb\u099a\u09cd\u099a \u09e8 \u0998\u09a3\u09cd\u099f\u09be \u09ac\u09be \u09a6\u09c7\u09b6\u09c7\u09b0 \u09aa\u09cd\u09b0\u099a\u09b2\u09bf\u09a4 \u09b6\u09cd\u09b0\u09ae \u0986\u0987\u09a8 \u09ae\u09cb\u09a4\u09be\u09ac\u09c7\u0995\u0964 <br>\n    \n    <b>\u0996) \u0993\u09ad\u09be\u09b0 \u099f\u09be\u0987\u09ae \u09b9\u09bf\u09b8\u09be\u09ac\u0983</b>\u0987\u09b9\u09be \u09ae\u09c2\u09b2 \u09ac\u09c7\u09a4\u09a8\u09c7\u09b0 \u09a6\u09cd\u09ac\u09bf\u0997\u09c1\u09a3 \u09b9\u09be\u09b0\u09c7 \u09b9\u09bf\u09b8\u09be\u09ac \u0995\u09b0\u09be \u09b9\u09df\u0964 \u09b9\u09bf\u09b8\u09be\u09ac\u0983 (\u09ae\u09c2\u09b2\u09ac\u09c7\u09a4\u09a8 / \u09e8\u09e6\u09ee) x \u09e8 \u09ae\u09cb\u099f \u0993\u09ad\u09be\u09b0\u099f\u09be\u0987\u09ae \u0998\u09a3\u09cd\u099f\u09be\u0964 <br>\n     \n    <b>\u09e9) \u099b\u09c1\u099f\u09bf\u0983</b> \u0995) \u09b8\u09be\u09aa\u09cd\u09a4\u09be\u09b9\u09bf\u0995 \u099b\u09c1\u099f\u09bf\u0983 \u09aa\u09cd\u09b0\u09a4\u09bf \u09e6\u09ed (\u09b8\u09be\u09a4) \u09e6\u09e7 (\u098f\u0995) \u09a6\u09bf\u09a8 \u09b8\u09be\u09aa\u09cd\u09a4\u09be\u09b9\u09bf\u0995 \u099b\u09c1\u099f\u09bf \u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u09be \u09b9\u09ac\u09c7\u0964 <br>\n    <b>\u0996) \u0989\u09ce\u09b8\u09ac \u099b\u09c1\u099f\u09bf\u0983</b> \u09ac\u099b\u09b0\u09c7 \u0995\u09ae\u09aa\u0995\u09cd\u09b7\u09c7 \u09e7\u09e7 (\u098f\u0997\u09be\u09b0) \u09a6\u09bf\u09a8 (\u09aa\u09c2\u09b0\u09cd\u09a3 \u09ac\u09c7\u09a4\u09a8\u09c7), \u09af\u09be\u09b9\u09be \u09ac\u09ce\u09b8\u09b0\u09c7\u09b0 \u09aa\u09cd\u09b0\u09a5\u09ae\u09c7\u0987 \u0995\u09cd\u09af\u09be\u09b2\u09c7\u09a8\u09cd\u09a1\u09be\u09b0 \u09ae\u09cb\u09a4\u09be\u09ac\u09c7\u0995 \u09a8\u09bf\u09b0\u09cd\u09a7\u09be\u09b0\u09a3 \u0995\u09b0\u09c7 \u09a8\u09cb\u099f\u09bf\u09b6 \u09ac\u09cb\u09b0\u09cd\u09a1\u09c7 \u09a6\u09c7\u09df\u09be \u09b9\u09df\u0964 <br>\n    <b>\u0996) \u09a8\u09c8\u09ae\u09bf\u09a4\u09cd\u09a4\u09bf\u0995 \u099b\u09c1\u099f\u09bf</b>\u0983 \u09ac\u099b\u09b0\u09c7 \u09e7\u09e6 (\u09a6\u09b6) \u09a6\u09bf\u09a8 (\u09aa\u09c2\u09b0\u09cd\u09a3 \u09ac\u09c7\u09a4\u09a8\u09c7)\u0964 <br>\n    <b>\u0997) \u09ac\u09be\u09b0\u09cd\u09b7\u09bf\u0995 \u099b\u09c1\u099f\u09bf:</b>\u0983 \u09e7 (\u098f\u0995) \u09ac\u09ce\u09b8\u09b0 \u098f\u0987 \u09ab\u09cd\u09af\u09be\u0995\u09cd\u099f\u09b0\u09bf\u09a4\u09c7 \u0985\u09ac\u09bf\u099a\u09cd\u099b\u09bf\u09a8\u09cd\u09a8\u09ad\u09be\u09ac\u09c7 \u099a\u09be\u0995\u09c1\u09b0\u09c0 \u0995\u09b0\u09be\u09b0 \u09aa\u09b0 \u0986\u09aa\u09a8\u09bf \u09aa\u09cd\u09b0\u09a4\u09bf \u09e7\u09ee (\u0986\u09a0\u09be\u09b0) \u0995\u09b0\u09cd\u09ae\u09a6\u09bf\u09ac\u09b8\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09e7 \u09a6\u09bf\u09a8 \u0995\u09b0\u09c7 \u09aa\u09c2\u09b0\u09cd\u09a3 \u09ac\u09c7\u09a4\u09a8\u09c7 \u09ac\u09be\u09b0\u09cd\u09b7\u09bf\u0995 \u099b\u09c1\u099f\u09bf \u09ad\u09cb\u0997 \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8\u0964 \u0986\u09b0 \u098f\u0987 \u099b\u09c1\u099f\u09bf \u09b8\u09b0\u09cd\u09ac\u09cb\u099a\u09cd\u099a \u09ea\u09e6 \u099f\u09bf \u099c\u09ae\u09be \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8\u0964 \u09ac\u09ce\u09b8\u09b0\u09c7 \u0985\u09b0\u09cd\u099c\u09bf\u09a4 \u099b\u09c1\u099f\u09bf\u09b0 \u0985\u09b0\u09cd\u09a7\u09c7\u0995\u09c7\u09b0 \u0985\u09a7\u09bf\u0995 \u09a8\u0997\u09a6\u09be\u09df\u09a8 \u0995\u09b0\u09be \u09af\u09be\u0987\u09ac\u09c7 \u09a8\u09be \u098f\u09ac\u0982 \u09ac\u09ce\u09b8\u09b0\u09c7 \u09ae\u09be\u09a4\u09cd\u09b0 \u098f\u0995\u09ac\u09be\u09b0 \u09a8\u0997\u09a6\u09be\u09df\u09a8 \u0995\u09b0\u09be \u09af\u09be\u0987\u09ac\u09c7\u0964 <br>\n    <b>\u0998) \u099a\u09bf\u0995\u09bf\u09ce\u09b8\u09be \u099b\u09c1\u099f\u09bf: </b>\u0986\u09aa\u09a8\u09bf \u09ac\u099b\u09b0\u09c7 \u09b8\u09b0\u09cd\u09ac\u09cb\u099a\u09cd\u099a \u09e7\u09ea (\u099a\u09cc\u09a6\u09cd\u09a6) \u09a6\u09bf\u09a8 \u09aa\u09c2\u09b0\u09cd\u09a3 \u09ac\u09c7\u09a4\u09a8\u09c7 \u098f\u0987 \u099b\u09c1\u099f\u09bf \u0995\u09be\u099f\u09be\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8\u0964 \u098f\u0995\u09cd\u09b7\u09c7\u09a4\u09cd\u09b0\u09c7 \u09aa\u09cd\u09b0\u09df\u09cb\u099c\u09a8\u09c0\u09df \u09a1\u09be\u0995\u09cd\u09a4\u09be\u09b0\u09bf \u09b8\u09be\u09b0\u09cd\u099f\u09bf\u09ab\u09bf\u0995\u09c7\u099f \u099c\u09ae\u09be \u09a6\u09bf\u09ac\u09c7\u09a8\u0964 <br>\n    <b>\u0999) \u09ae\u09be\u09a4\u09c3\u0995\u09be\u09b2\u09c0\u09a8 \u099b\u09c1\u099f\u09bf\u0983</b>\u09ae\u09b9\u09bf\u09b2\u09be \u09b6\u09cd\u09b0\u09ae\u09bf\u0995 \u0995\u09be\u09b0\u0996\u09be\u09a8\u09be\u09df \u098f\u0995 \u09a8\u09be\u0997\u09be\u09b0\u09c7 \u09e6\u09ec (\u099b\u09df) \u09ae\u09be\u09b8 \u0995\u09be\u099c \u0995\u09b0\u09b2\u09c7 \u09e8\u09e6\u09e6\u09ec \u09b8\u09be\u09b2\u09c7\u09b0 \u09ae\u09be\u09a4\u09c3\u0995\u09be\u09b2\u09c0\u09a8 \u0986\u0987\u09a8\u09c7\u09b0 \u09a7\u09be\u09b0\u09be \u0985\u09a8\u09c1\u09af\u09be\u09df\u09c0 \u09e7\u09ec \u09b8\u09aa\u09cd\u09a4\u09be\u09b9 \u09ac\u09be \u09e7\u09e7\u09e8 (\u098f\u0995\u09b6\u09a4 \u09ac\u09be\u09b0\u09cb) \u09a6\u09bf\u09a8\u09c7\u09b0 \u099b\u09c1\u099f\u09bf \u09ad\u09cb\u0997 \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8\u0964 <br>\n    <br>\n    <b>\u099a\u09be\u0995\u09c1\u09b0\u09c0\u09b0 \u09b6\u09b0\u09cd\u09a4\u0983</b><br>\n    \u0995) \u0986\u09aa\u09a8\u09bf \u099a\u09be\u0995\u09c1\u09b0\u09c0\u09b0 \u09aa\u09cd\u09b0\u09a5\u09ae \u09e9 (\u09a4\u09bf\u09a8) \u09ae\u09be\u09b8 \u09b6\u09bf\u0995\u09cd\u09b7\u09be\u09a8\u09ac\u09bf\u09b6 \u09b9\u09bf\u09b8\u09be\u09ac\u09c7 \u09a5\u09be\u0995\u09ac\u09c7\u09a8 \u098f\u09ac\u0982 \u0986\u09aa\u09a8\u09be\u09b0 \u0995\u09be\u099c \u09b8\u09a8\u09cd\u09a4\u09cb\u09b7\u099c\u09a8\u0995 \u09a8\u09be \u09b9\u09b2\u09c7 \u0989\u0995\u09cd\u09a4 \u09b8\u09ae\u09df \u0995\u09be\u09b2 \u0986\u09b0\u0993)\u09e9 (\u09a4\u09bf\u09a8) \u09ae\u09be\u09b8 \u09ac\u09b0\u09cd\u09a7\u09bf\u09a4 \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7\u0964 \u09ac\u09b0\u09cd\u09a7\u09bf\u09a4 \u09e9 (\u09a4\u09bf\u09a8) \u09ae\u09be\u09b8\u09c7 \u09af\u09a6\u09bf \u0986\u09aa\u09a8\u09bf \u0995\u09be\u099c\u09c7 \u09a6\u0995\u09cd\u09b7\u09a4\u09be \u09a6\u09c7\u0996\u09be\u09a4\u09c7 \u09a8\u09be \u09aa\u09be\u09b0\u09c7\u09a8, \u09a4\u09ac\u09c7 \u0995\u09cb\u09a8 \u09ac\u09bf\u099c\u09cd\u099e\u09aa\u09cd\u09a4\u09bf \u099b\u09be\u09dc\u09be\u0987 \u0986\u09aa\u09a8\u09be\u0995\u09c7 \u099a\u09be\u0995\u09c1\u09b0\u09c0 \u09a5\u09c7\u0995\u09c7 \u0985\u09ac\u09cd\u09af\u09be\u09b9\u09a4\u09bf \u09a6\u09c7\u09df\u09be \u09b9\u09ac\u09c7\u0964 \u09a4\u09ac\u09c7 \u09b6\u09b0\u09cd\u09a4 \u09a5\u09be\u0995\u09c7 \u09af\u09c7, \u09b6\u09bf\u0995\u09cd\u09b7\u09be\u09a8\u09ac\u09bf\u09b6 \u0995\u09be\u09b2 \u09a4\u09bf\u09a8 \u09ae\u09be\u09b8 \u09ae\u09c7\u09df\u09be\u09a6 \u09ac\u09c3\u09a6\u09cd\u09a7\u09bf \u09b6\u09c7\u09b7\u09c7 \u0995\u09a8\u09ab\u09be\u09b0\u09ae\u09c7\u09b6\u09a8 \u09b2\u09c7\u099f\u09be\u09b0 \u09a6\u09c7\u09df\u09be \u09a8\u09be \u09b9\u09b2\u09c7\u0993 \u0986\u09aa\u09a8\u09bf \u09b8\u09cd\u09a5\u09be\u09df\u09c0 \u09ac\u09b2\u09c7 \u0997\u09a3\u09cd\u09af \u09b9\u09ac\u09c7\u09a8\u0964 <br>\n    \n    \u0996) \u09aa\u09b0\u09bf\u099a\u09df \u09aa\u09a4\u09cd\u09b0\u0983 \u0986\u09aa\u09a8\u09be\u0995\u09c7 \u09aa\u09b0\u09bf\u099a\u09df \u09aa\u09a4\u09cd\u09b0 \u09a6\u09c7\u09df\u09be \u09b9\u09ac\u09c7, \u09af\u09be\u09b9\u09be \u09b8\u09b0\u09cd\u09ac\u09be\u0995\u09cd\u09b7\u09a3\u09bf\u0995 \u0986\u09aa\u09a8\u09be\u09b0 \u09b8\u09be\u09a5\u09c7 \u09b0\u09be\u0996\u09ac\u09c7\u09a8\u0964 \u098f\u099b\u09be\u09dc\u09be \u0986\u09aa\u09a8\u09bf \u0995\u09be\u09b0\u0996\u09be\u09a8\u09be\u09df \u09aa\u09cd\u09b0\u09ac\u09c7\u09b6\u09c7\u09b0 \u09aa\u09c2\u09b0\u09cd\u09ac\u09c7 \u09aa\u09be\u099e\u09cd\u099a \u0995\u09b0\u09ac\u09c7\u09a8 \u098f\u09ac\u0982 \u09ac\u09c7\u09b0 \u09b9\u0993\u09df\u09be\u09b0 \u09b8\u09ae\u09df \u0986\u09aa\u09a8\u09be\u09b0 \u09b8\u09a0\u09bf\u0995 \u0995\u09be\u09b0\u09cd\u09af\u09cd\u09af\u09b8\u09ae\u09df \u09ac\u09c1\u099d\u09c7 \u0986\u0987\u09a1\u09bf \u0995\u09be\u09b0\u09cd\u09a1 \u09aa\u09be\u099e\u09cd\u099a \u0995\u09b0\u09ac\u09c7\u09a8 \u098f\u09ac\u0982 \u0989\u0995\u09cd\u09a4 \u09b9\u09be\u099c\u09bf\u09b0\u09be \u09a5\u09c7\u0995\u09c7 \u0986\u09aa\u09a8\u09be\u09b0 \u09ae\u09be\u09b8\u09bf\u0995 \u09b9\u09be\u099c\u09bf\u09b0\u09be \u098f\u09ac\u0982 \u0985\u09a4\u09bf\u09b0\u09bf\u0995\u09cd\u09a4 \u0995\u09be\u099c\u09c7\u09b0 \u09b8\u09ae\u09df \u09a8\u09bf\u09b0\u09cd\u09a7\u09be\u09b0\u09a3 \u0995\u09b0\u09be \u09b9\u09ac\u09c7\u0964 <br>\n    \n    \u0997) \u09aa\u09cd\u09b0\u09ac\u09c7\u09b6\u09a8 \u09aa\u09bf\u09b0\u09bf\u09df\u09a1\u09c7 \u0995\u09cb\u09ae\u09cd\u09aa\u09be\u09a8\u09bf \u09af\u09c7 \u0995\u09cb\u09a8 \u09b8\u09ae\u09df \u09af\u09c7 \u0995\u09cb\u09a8 \u09aa\u09cd\u09b0\u0995\u09be\u09b0 \u0995\u09be\u09b0\u09a3 \u09a6\u09b0\u09cd\u09b6\u09be\u09a8\u09cb \u09ac\u09cd\u09af\u09a4\u09bf\u09b0\u09c7\u0995\u09c7 \u0986\u09aa\u09a8\u09be\u09b0 \u099a\u09be\u0995\u09c1\u09b0\u09c0\u09b0\u09b0 \u0985\u09ac\u09b8\u09be\u09a8 \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8\u0964 \u0985\u09a5\u09ac\u09be \u0986\u09aa\u09a8\u09bf \u0993 \u099a\u09be\u0995\u09c1\u09b0\u09c0 \u09a5\u09c7\u0995\u09c7 \u0987\u09b8\u09cd\u09a4\u09c7\u09ab\u09be \u09a8\u09bf\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8 \u0964 <br>\n    \u0998) \u099a\u09be\u0995\u09c1\u09b0\u09c0\u09a4\u09c7 \u09b8\u09cd\u09a5\u09be\u09df\u09c0 \u09b9\u09ac\u09be\u09b0 \u09aa\u09b0 \u0986\u09aa\u09a8\u09bf \u09b8\u09cd\u09ac\u09c7\u099a\u09cd\u099b\u09be\u09df \u099a\u09be\u0995\u09c1\u09b0\u09c0 \u09b9\u09a4\u09c7 \u09aa\u09a6\u09a4\u09cd\u09af\u09be\u0997 \u0995\u09b0\u09a4\u09c7 \u099a\u09be\u0987\u09b2\u09c7 \u09ec\u09e6 (\u09b7\u09be\u099f) \u09a6\u09bf\u09a8 \u09aa\u09c2\u09b0\u09cd\u09ac\u09c7 \u09b2\u09bf\u0996\u09bf\u09a4 \u09a8\u09cb\u099f\u09bf\u09b6 \u0995\u09b0\u09cd\u09a4\u09c3\u09aa\u0995\u09cd\u09b7\u0995\u09c7 \u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u09a4\u09c7 \u09b9\u09ac\u09c7\u0964 <br>\n    \n    \u0999) \u0986\u09aa\u09a8\u09be\u09b0 \u099a\u09be\u0995\u09c1\u09b0\u09c0 \u0995\u09cb\u09ae\u09cd\u09aa\u09be\u09a8\u09c0 \u0995\u09b0\u09cd\u09a4\u09c3\u09aa\u0995\u09cd\u09b7 \u099c\u09be\u09b0\u09c0\u0995\u09c3\u09a4 \u09ac\u09bf\u09a7\u09bf-\u09ac\u09bf\u09a7\u09be\u09a8 \u0993 \u09ac\u09be\u0982\u09b2\u09be\u09a6\u09c7\u09b6\u09c7\u09b0 \u09aa\u09cd\u09b0\u099a\u09b2\u09bf\u09a4 \u09b6\u09cd\u09b0\u09ae \u0986\u0987\u09a8 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u09aa\u09b0\u09bf\u099a\u09be\u09b2\u09bf\u09a4 \u09b9\u09ac\u09c7\u0964 \u0995\u09cb\u09ae\u09cd\u09aa\u09be\u09a8\u09c0\u09b0 \u09af\u09be\u09ac\u09a4\u09c0\u09df \u09a8\u09bf\u09df\u09ae\u0995\u09be\u09a8\u09c1\u09a8 \u09aa\u09b0\u09bf\u09ac\u09b0\u09cd\u09a4\u09a8 \u09af\u09cb\u0997\u09cd\u09af \u098f\u09ac\u0982 \u0986\u09aa\u09a8\u09bf \u09aa\u09b0\u09bf\u09ac\u09b0\u09cd\u09a4\u09bf\u09a4 \u09a8\u09bf\u09df\u09ae \u0995\u09be\u09a8\u09c1\u09a8 \u09b8\u09b0\u09cd\u09ac\u09a6\u09be \u09ae\u09c7\u09a8\u09c7 \u099a\u09b2\u09a4\u09c7 \u09ac\u09be\u09a7\u09cd\u09af \u09a5\u09be\u0995\u09ac\u09c7\u09a8 \u0964 \u0986\u09aa\u09a8\u09bf \u09af\u09a6\u09bf \u0995\u0996\u09a8\u0993 \u0995\u09cb\u09a8\u09b0\u09c2\u09aa \u0985\u09b8\u09a6\u09be\u099a\u09b0\u09a3\u09c7\u09b0 \u0985\u09aa\u09b0\u09be\u09a7\u09c7 \u09a6\u09cb\u09b7\u09c0 \u09aa\u09cd\u09b0\u09ae\u09be\u09a3\u09bf\u09a4 \u09b9\u09a8, \u09a4\u09ac\u09c7 \u0995\u09b0\u09cd\u09a4\u09c3\u09aa\u0995\u09cd\u09b7\u0995\u09c7 \u0986\u0987\u09a8 \u09ae\u09cb\u09a4\u09be\u09ac\u09c7\u0995 \u0986\u09aa\u09a8\u09be\u0995\u09c7 \u099a\u09be\u0995\u09c1\u09b0\u09c0\u099a\u09cd\u09af\u09c1\u09a4\u09bf\u09b8\u09b9 \u09af\u09c7 \u0995\u09cb\u09a8 \u09a7\u09b0\u09a8\u09c7\u09b0 \u09b6\u09be\u09b8\u09cd\u09a4\u09bf \u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8\u0964 <br>\n    \n       \n    \u099a) \u0995\u09b0\u09cd\u09a4\u09c3\u09aa\u0995\u09cd\u09b7 \u09aa\u09cd\u09b0\u09df\u09cb\u099c\u09a8 \u09ac\u09cb\u09a7\u09c7 \u0986\u09aa\u09a8\u09be\u0995\u09c7 \u098f\u0987 \u09aa\u09cd\u09b0\u09a4\u09bf\u09b7\u09cd\u09a0\u09be\u09a8\u09c7\u09b0 \u09af\u09c7 \u0995\u09cb\u09a8 \u09ac\u09bf\u09ad\u09be\u0997\u09c7 \u0985\u09a5\u09ac\u09be \u09ac\u09be\u0982\u09b2\u09be\u09a6\u09c7\u09b6\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u098f \u09aa\u09cd\u09b0\u09a4\u09bf\u09b7\u09cd\u09a0\u09be\u09a8\u09c7\u09b0 \u09af\u09c7 \u0995\u09cb\u09a8 \u0995\u09be\u09b0\u0996\u09be\u09a8\u09be\u09df/ \u0985\u09ab\u09bf\u09b8\u09c7 \u09ac\u09a6\u09b2\u09bf \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8\u0964 \u098f\u0987 \u0995\u09cb\u09ae\u09cd\u09aa\u09be\u09a8\u09c0\u09b0 \u09b8\u09b9\u09bf\u09a4 \u0986\u09aa\u09a8\u09be\u09b0 \u09a6\u09c0\u09b0\u09cd\u0996 \u0993 \u0986\u09a8\u09a8\u09cd\u09a6\u09ae\u09df \u09ad\u09ac\u09bf\u09b7\u09cd\u09af\u09ce\u0987 \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u098f\u0995\u09be\u09a8\u09cd\u09a4 \u0995\u09be\u09ae\u09cd\u09af\u0964 <br>\n    \u0986\u09ae\u09bf \u098f\u0987 \u09a8\u09bf\u09df\u09cb\u0997\u09aa\u09a4\u09cd\u09b0\u09c7\u09b0 \u09b6\u09b0\u09cd\u09a4\u09be\u09ac\u09b2\u09c0 \u09aa\u09be\u09a0 \u0995\u09b0\u09c7\u099b\u09bf / \u0986\u09ae\u09be\u0995\u09c7 \u09aa\u09be\u09a0 \u0995\u09b0\u09c7 \u09b6\u09c1\u09a8\u09be\u09a8\u09cb \u09b9\u09df\u09c7\u099b\u09c7\u0964 \u098f\u09a4\u09c7 \u09ac\u09b0\u09cd\u09a3\u09bf\u09a4 \u09b6\u09b0\u09cd\u09a4\u09be\u09a6\u09bf \u09b8\u09ae\u09cd\u09aa\u09c2\u09b0\u09cd\u09a3\u09b0\u09c2\u09aa\u09c7 \u0985\u09ac\u0997\u09a4 \u09b9\u09df\u09c7, \u0986\u09ae\u09bf \u09b8\u09cd\u09ac\u09c7\u099a\u09cd\u099b\u09be\u09df \u0989\u09aa\u09b0\u09cb\u0995\u09cd\u09a4 \u09b6\u09b0\u09cd\u09a4\u09b8\u09ae\u09c2\u09b9 \u09ae\u09c7\u09a8\u09c7, \u09a8\u09bf\u09df\u09cb\u0997\u09aa\u09a4\u09cd\u09b0\u09c7 \u09b8\u09cd\u09ac\u09be\u0995\u09cd\u09b7\u09b0 \u0995\u09b0\u09c7 \u0985\u09a8\u09c1\u09b2\u09bf\u09aa\u09bf \u0997\u09cd\u09b0\u09b9\u09a3 \u0995\u09b0\u09c7\u099b\u09bf \u098f\u09ac\u0982{{doc.company}} \u098f \u0989\u09aa\u09b0\u09c7 \u09ac\u09b0\u09cd\u09a3\u09bf\u09a4 \u09a4\u09be\u09b0\u09bf\u0996\u09c7 \u0995\u09be\u099c\u09c7 \u09af\u09cb\u0997\u09a6\u09be\u09a8 \u0995\u09b0\u09c7\u099b\u09bf\u0964 \n    \n    </p>\n    \n    <br>\n    <p class=\"signature\">\n        _____________________ <br>\n        \u09aa\u09cd\u09b0\u09b6\u09be\u09b8\u09a8\u09bf\u0995 \u09aa\u09cd\u09b0\u09a7\u09be\u09a8/\u09ac\u09cd\u09af\u09be\u09ac\u09b8\u09cd\u09a5\u09be\u09aa\u0995  \n    </p>\n    <br>\n    \n    <p>\n        \u0986\u09ae\u09bf \u098f\u0987 \u09a8\u09bf\u09df\u09cb\u0997\